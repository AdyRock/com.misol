{
    "title": {
        "en": "The PM2.5 air quality has become",
        "nl": "De PM2.5 luchtkwaliteit is geworden"
    },
    "titleFormatted": {
        "en": "The PM2.5 air quality has become [[compare_type]] [[measure_aq]]",
        "nl": "De PM2.5 luchtkwaliteit is [[compare_type]] [[measure_aq]] geworden"
    },
    "args": [
        {
            "name": "device",
            "type": "device",
            "filter": "driver_id=pm25|pm10"
        },
        {
            "type": "dropdown",
            "name": "compare_type",
            "title": {
                "en": "Comparison type",
                "nl": "Soort vergelijking"
            },
            "values": [
                {
                    "id": "<=",
                    "label": {
                        "en": "better than or equal to",
                        "nl": "beter dan of gelijk aan"
                    }
                },
                {
                    "id": "==",
                    "label": {
                        "en": "equal to",
                        "nl": "gelijk aan"
                    }
                },
                {
                    "id": ">=",
                    "label": {
                        "en": "worse than or equal to",
                        "nl": "slechter dan of gelijk aan"
                    }
                }
            ]
        },
        {
            "type": "dropdown",
            "name": "measure_aq",
            "title": {
                "en": "Minimum Air quality",
                "nl": "Minimale luchtkwaliteit"
            },
            "values": [
                {
                    "id": "0",
                    "label": {
                        "en": "Good",
                        "nl": "Goed"
                    }
                },
                {
                    "id": "1",
                    "label": {
                        "en": "Moderate",
                        "nl": "Gematigd"
                    }
                },
                {
                    "id": "2",
                    "label": {
                        "en": "Slightly Unhealthy",
                        "nl": "Enigszins ongezond"
                    }
                },
                {
                    "id": "3",
                    "label": {
                        "en": "Unhealthy",
                        "nl": "Ongezond"
                    }
                },
                {
                    "id": "4",
                    "label": {
                        "en": "Very Unhealthy",
                        "nl": "Heel ongezond"
                    }
                },
                {
                    "id": "5",
                    "label": {
                        "en": "Hazardous",
                        "nl": "Gevaarlijk"
                    }
                }
            ]
        }
    ],
    "tokens": [
        {
            "name": "measure_aq_name",
            "type": "string",
            "title": {
                "en": "PM2.5 Air Quality",
                "nl": "PM2.5 Luchtkwaliteit"
            },
            "example": {
                "en": "Moderate",
                "nl": "Gematigd"
            }
        },
        {
            "name": "measure_aq_item",
            "type": "number",
            "title": {
                "en": "Air Quality item number",
                "nl": "Luchtkwaliteit artikelnummer"
            },
            "example": {
                "en": "0 to 5 = Good to Hazardous",
                "nl": "Gematigd"
            }
        }
    ]
}